        "inputs": {
          "buildCommand": "cd frontend && npm install && npm run build",
          "outputPath": "frontend/dist",
          "cloudPath": "/",
          "envVariables": {
            "VITE_SUPABASE_URL": "https://rqgvxjltlupikmzxvqrc.supabase.co",
            "VITE_SUPABASE_ANON_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6InJxZ3Z4amx0bHVwaWttenh2cXJjIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTM0OTg0NTUsImV4cCI6MjA2OTA3NDQ1NX0.HAqkkklscl-dWTv8mUzBtIavHdQA1_5X5OGAcWtch6g"
